#!/usr/bin/env python
# coding: utf-8

# In[4]:


import scipy.io
import numpy as np
import matplotlib.pyplot as plt



# In[8]:


file = '/home/cyberpunklabs/proyectos/SERT/SERT938.mat'

data = scipy.io.loadmat(file)
dd = data['SERT'][0][0]

r = len(dd[0][0])
c = len(dd[0])

matrix = np.zeros((r, c))

for c in range(c):
    matrix[:, c] = dd[0][c]



# In[ ]:


np.savetxt("raton1.csv", matrix, delimiter=",")


# In[16]:



matrix = np.genfromtxt('raton1.csv', delimiter=',')


# In[6]:


for p in range(10):
    plt.subplot(10,1,p+1)
    #xticks([]), yticks([])
    #title('subplot(2,1,1)')
    plt.plot(matrix[p,:])


plt.show()


